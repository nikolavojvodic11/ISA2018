<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity FlightTicket.
    -->
    <changeSet id="20190110011415-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="flight_id"
                                 baseTableName="flight_ticket"
                                 constraintName="fk_flight_ticket_flight_id"
                                 referencedColumnNames="id"
                                 referencedTableName="flight"/>

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="flight_ticket"
                                 constraintName="fk_flight_ticket_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="isa_user"/>

    </changeSet>
</databaseChangeLog>
